{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchDatasources": ["docker"],
      "matchPackagePatterns": ["postgresql"],
      "allowedVersions": "<17"
    },
    {
      "matchDatasources": ["docker"],
      "matchPackagePatterns": ["teslamate"],
      "allowedVersions": "<1.31"
    },
    {
      "matchDatasources": ["docker"],
      "matchPackagePatterns": ["qbittorrent"],
      "allowedVersions": "<5"
    }
  ]
}
